
fn main(): bool
{
	// Read a file of known size for exactly its number of lines and verify
	// eof() is false, then read one more line and verify eof() is true

	let stro =
	[
		"line 0",
		"line aoeu htns gcrl qjkx",
		"line 1",
		"line htns xbmw",
		"line final"
	];

	let filename = "build/test-03.txt";

	// write
	let fo = open(filename);
	for i in [0: size(stro, 0)]
		writeln(fo, stro[i]);
	close(fo);

	// read
	let fi = open(filename);
	//println("fi = " + str(fi));

	let str = "";

	for i in [0: size(stro, 0)]
	{
		str = readln(fi);
		//println(str);
	}

	let eof1 = eof(fi);
	//println("eof1 = ", eof1);

	str = readln(fi);
	//println(str);

	let eof2 = eof(fi);
	//println("eof2 = ", eof2);

	close(fi);
	//println("ending main");

	let ans = not eof1 and eof2;
	//println("ans = ", ans);
	return ans;
}

return main();

