
fn add_fn(u: [i32; :], v: [i32; :]): [i32; :]
//fn add_fn(u: [i32; :], v: [i32; :]): i32
{
	// Return w = u + v
	//
	// Don't use this in production

	let w = [0; size(u,0)];
	//let w = 0;

	for i in [0: size(u,0)]
	{
		//w = u[i] + v[i];
		w[i] = u[i] + v[i];
	}
	return w;
}

let n = 3;

let a = [0: n];
let b = [n+1: -1: 1];

let ans = add_fn(a, b);
return ans;

