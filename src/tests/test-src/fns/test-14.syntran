
fn add_fn(u: [f32; :], v: [f32; :]): [f32; :]
{
	// Return w = u + v
	//
	// Don't use this in production

	let w = [0.0f; size(u,0)];

	for i in [0: size(u,0)]
		w[i] = u[i] + v[i];
	return w;
}

let n = 3;

let a = [0.0f: 1.0f * (n - 1); n];
let b = [1.0f * (n+1): -1.0f: 1.5f];

a;
b;

let ans = add_fn(a, b);
return ans;

