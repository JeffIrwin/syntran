
// Anonymous global block
{

	// There's no scoping yet in 0.0.5.  Everything is global
	{
		let foo = 1
		let bar = 2
	}

	let baz = 4

	baz = 3

	foo + bar * baz

	{
		let p = true
	}

	let q = false

	p or q
	not q and foo + bar == baz

	//// The final (uncommented) statement of the main block is the printed
	//// result
	//foo + bar * baz

}

